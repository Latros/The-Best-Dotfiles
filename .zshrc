export ZSH=/home/sam/.oh-my-zsh
ZSH_THEME="robbyrussell"
plugins=(git)
export PATH="$HOME/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games:/usr/local/games"
source $ZSH/oh-my-zsh.sh
source ~/z.sh
alias cb="git checkout -B"
alias mb="git merge"
alias sb="git checkout"
alias db="git branch -D"
alias gc="git add --all; git commit -am"
alias push="git push"
alias pull="git pull"
alias stat="git status"
alias c="clear"
alias diff="git diff"
alias lb="git branch --list"
alias npmi="sudo npm install"
alias npmig="sudo npm install -g"
alias node="nodejs"
alias aliases="subl ~/.zshrc"
alias killport="fuser -n tcp -k"
alias lg="git lg"
alias go="killport 9001; grunt go"
alias clone="git clone"
alias fixgitignore="git rm -r --cached .; git add --all"
alias syncfork="git checkout master; git fetch upstream; git merge upstream/master; git push"
alias syncbsfork="git checkout master; git fetch upstream; git merge upstream/stageing; git push"
alias al="aliases"
alias gd="git diff"
alias repl="lein repl"
alias serv="lein ring server-headless"
alias sf="syncfork"
alias goc="killport 3000; lein ring server"
alias s="!!"
alias runapi="docker run -it --privileged=true -v /var/lib/docker:/var/lib/docker -p 9080:9080 --name bs-api docker.liveopslabs.com/bs-api"
alias npmminor="npm version minor;"
alias npmpatch="npm version patch;"
alias npmmajor="npm version major;"
alias bowerminor="bower version minor;"
alias bowerpatch="bower version patch;"
alias bowermajor="bower version major;"
alias sd="docker stop $(docker ps -a -q); docker rm $(docker ps -a -q)"
alias ud="docker pull docker.liveopslabs.com/bs-api"
alias fig="killport 3449; lein figwheel"